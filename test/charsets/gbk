	window.location='/userRegEdit.do';
}
//用户名的校验
function checkUserName(){
	var result = true;
	var name = document.getElementById("userName");
	var loginError = document.getElementById("loginError");
	if(!isSingleEmail(name.value)||lengthCheck(name.value, 5, 255)){
		loginError.innerHTML = "格式不正确×";
		loginError.className = "yanzheng_2";
		result = false;
	}else{
		loginError.innerHTML = "";
		loginError.className = "undefined";
	}
	return result;
}
//密码的校验
function checkPassword(){
	var result = true;
	var password = document.getElementById("password");
	var loginError = document.getElementById("loginError");
	//第一步，password应满足"只能为数字和英文字母"
	if(password.value.search(/[^a-zA-Z_0-9\s]/g) != -1){
		loginError.innerHTML = "格式不正确×";
		loginError.className = "yanzheng_2";
		result = false;
	}else{
		//第二步，password应满足"长度在6至32位之间"
    	if(password.value.length < 6 || password.value.length > 16){
    		loginError.innerHTML = "格式不正确×";
    		loginError.className = "yanzheng_2";
    		result = false;
    	}else{
    		loginError.innerHTML = "";
    		loginError.className = "undefined";
    	}
    }
	return result;
}
//email的校验
function isSingleEmail(s){
	var flag=true;
	var emailRegS=/^([a-zA-Z0-9_\-\.\+]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
	if (!emailRegS.exec(s))
		flag=false ;
	return flag ;
}
function keyDown(event){
	var saveRegUserBtn = document.getElementById("saveRegUserBtn");
	if (event.keyCode == 13){
		saveRegUserBtn.click();//使用表单提交
	}
}

function checkAll(){
	var result = true;
	if(!checkUserName())
		result = false;
	if(!checkPassword())
		result = false;
	return result;
}
